#: actions.cpp:237
#, c++-format
msgid "File \"{}\" on line {}: {}"
msgstr "Αρχείο «{}» στη γραμμή {}: {}"

#: actions.cpp:636 actions.cpp:642 actions.cpp:648 actions.cpp:654
msgid "\"HOOK:PKG_FILTER:DIRECTION:OPTIONS:CMD\" format expected"
msgstr "Αναμενόταν η μορφή «HOOK:PKG_FILTER:DIRECTION:OPTIONS:CMD»"

#: actions.cpp:676
#, c++-format
msgid "Unknown \"enabled\" option value \"{}\""
msgstr "Άγνωστη τιμή επιλογής «enabled»: «{}»"

#: actions.cpp:683
#, c++-format
msgid "Unknown option \"{}\""
msgstr "Άγνωστη επιλογή «{}»"

#: actions.cpp:711
#, c++-format
msgid "Unknown hook \"{}\""
msgstr "Άγνωστο άγκιστρο (hook) «{}»"

#: actions.cpp:720
msgid ""
"Package filter can only be used in GOAL_RESOLVED, PRE_TRANS and POST_TRANS "
"hooks"
msgstr ""
"Το φίλτρο πακέτων μπορεί να χρησιμοποιηθεί μόνο στα άγκιστρα GOAL_RESOLVED, "
"PRE_TRANS και POST_TRANS"

#: actions.cpp:726
msgid "Cannot use direction without package filter"
msgstr "Δεν είναι δυνατή η χρήση κατεύθυνσης (direction) χωρίς φίλτρο πακέτων"

#: actions.cpp:740
#, c++-format
msgid "Unknown package direction \"{}\""
msgstr "Άγνωστη κατεύθυνση πακέτου «{}»"

#: actions.cpp:747
#, c++-format
msgid "Unknown mode \"{}\""
msgstr "Άγνωστη λειτουργία (mode) «{}»"

#: actions.cpp:755
#, c++-format
msgid "Unsupported value of the \"raise_error\" option: {}"
msgstr "Μη υποστηριζόμενη τιμή της επιλογής «raise_error»: {}"

#: actions.cpp:760
msgid "Missing command"
msgstr "Λείπει η εντολή"

#: actions.cpp:953
#, c++-format
msgid "Synax error: Missing equal sign (=) in action output line: {}"
msgstr ""
"Συντακτικό σφάλμα: Λείπει το σύμβολο ισότητας (=) στη γραμμή εξόδου της "
"ενέργειας: {}"

#: actions.cpp:964
#, c++-format
msgid "Cannot set option: Action output line: {}"
msgstr "Αδυναμία ορισμού επιλογής: Γραμμή εξόδου ενέργειας: {}"

#: actions.cpp:982
#, c++-format
msgid "Action sent the wrong log level: {}"
msgstr "Η ενέργεια έστειλε εσφαλμένο επίπεδο καταγραφής: {}"

#: actions.cpp:987 actions.cpp:1794
#, c++-format
msgid "Action calls for stop: {}"
msgstr "Η ενέργεια ζητά διακοπή: {}"

#: actions.cpp:990 actions.cpp:1801
#, c++-format
msgid "Action sent error message: {}"
msgstr "Η ενέργεια έστειλε μήνυμα σφάλματος: {}"

#: actions.cpp:995
#, c++-format
msgid ""
"Syntax error: Action output line must start with \"tmp.\" or \"conf.\" or "
"\"var.\" or \"stop=\" or \"error=\": {}"
msgstr ""
"Συντακτικό σφάλμα: Η γραμμή εξόδου της ενέργειας πρέπει να ξεκινά με «tmp.», "
"«conf.», «var.», «stop=» ή «error=»: {}"

#: actions.cpp:1020
msgid "Error reading from action (from pipe)"
msgstr "Σφάλμα κατά την ανάγνωση από την ενέργεια (μέσω διοχέτευσης/pipe)"

#: actions.cpp:1038
#, c++-format
msgid "Syntax error in json request from action: Missing starting '{{' char"
msgstr ""
"Συντακτικό σφάλμα στο αίτημα json από την ενέργεια: Λείπει ο αρχικός "
"χαρακτήρας «{{»"

#: actions.cpp:1062
msgid "Error during processing of a request from action."
msgstr "Σφάλμα κατά την επεξεργασία αιτήματος από την ενέργεια."

#: actions.cpp:1071
#, c++-format
msgid "Syntax error in json request from action: {}"
msgstr "Συντακτικό σφάλμα στο αίτημα json από την ενέργεια: {}"

#: actions.cpp:1079
msgid "Syntax error in json request from action: Incomplete input"
msgstr "Συντακτικό σφάλμα στο αίτημα json από την ενέργεια: Ημιτελής είσοδος"

#: actions.cpp:1154
msgid "Cannot write response"
msgstr "Αδυναμία εγγραφής απόκρισης"

#: actions.cpp:1821
msgid "Actions plugin: Cannot create pipe"
msgstr "Πρόσθετο Actions: Αδυναμία δημιουργίας διοχέτευσης (pipe)"

#: actions.cpp:1913
msgid "Actions plugin: Cannot fork"
msgstr "Πρόσθετο Actions: Αδυναμία διακλάδωσης (fork)"

#: actions.cpp:1968
msgid "Actions plugin: Cannot bind command stdin"
msgstr "Πρόσθετο Actions: Αδυναμία δέσμευσης του stdin της εντολής"

#: actions.cpp:1970
msgid "Actions plugin: Cannot bind command stdout"
msgstr "Πρόσθετο Actions: Αδυναμία δέσμευσης του stdout της εντολής"

#: actions.cpp:1988
#, c++-format
msgid "Cannot execute action, command \"{}\" arguments \"{}\""
msgstr "Αδυναμία εκτέλεσης ενέργειας, εντολή «{}» ορίσματα «{}»"

#: actions.cpp:1996
msgid "Error during preparation child process"
msgstr "Σφάλμα κατά την προετοιμασία της θυγατρικής διεργασίας"

#: actions.cpp:2015
#, c++-format
msgid "Exit code: {}"
msgstr "Κωδικός εξόδου: {}"

#: actions.cpp:2019
#, c++-format
msgid "Terminated by signal: {}"
msgstr "Τερματίστηκε από σήμα: {}"
